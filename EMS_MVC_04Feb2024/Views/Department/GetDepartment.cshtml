@model IEnumerable<EMS_MVC_04Feb2024.Models.Department.Department>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
</head>
<body>
    <h2>Hello from Department Controller</h2>
    <table>
        <thead>
            <tr>
                <th>Department Id</th>
                <th>Department Code</th>
                <th>Department Name</th>
            </tr>
        </thead>
        <tbody>
           @foreach (var item in Model)
           {
            <tr>
                <td>@item.DepartmentId</td>
                <td>@item.DepartmentCode</td>
                <td>@item.DepartmentName</td>
            </tr>
           }
        </tbody>
    </table>
</body>
</html>

