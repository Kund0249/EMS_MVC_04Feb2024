@model IEnumerable<EMS_MVC_04Feb2024.Models.Department.DepartmentModel>



<div class="row">
    <div class="col-md-6 offset-3">
        <table class="table">
            <thead>
                <tr>
                    @*<th>Department Id</th>*@
                    <th>Department Code</th>
                    <th>Department Name</th>
                    <th>Action</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    @*<tr>
                        <td>@item.DepartmentId</td>
                        <td>@item.DepartmentCode</td>
                        <td>@item.DepartmentName</td>
                    </tr>*@
                    @Html.Partial("_DepartmentList", item)
                }
            </tbody>
        </table>
    </div>
</div>


